    "nextjs",
    "npm",
    "module",
    "node",
    "promise",
    "correios",
    "busca",
    "correios-rastreio",
    "rastreio",
    "rastreamento",
    "vercel",
    "vercel-serverless-functions"
  ],
  "author": "Hebert F Barros <tecnobertinfo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/rastrearpedidos/issues"
  },
  "homepage": "https://rastrearpedidos.com.br",
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
